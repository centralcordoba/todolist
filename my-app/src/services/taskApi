import axios from "axios";

import { ITask } from '../Interfaces';

export default{
    list:async ():Promise<ITask[]> => {
        return axios.get(`https://https://localhost:7280/todotask`)
        .then((response) =>{
            console.log("data loading");
            console.log(response.data);
            return response.data;
        })
        
    },
}